@model IEnumerable<ABCShoppingMall.Models.Ticket>

<!DOCTYPE html>

<html>
<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link href="https://fonts.googleapis.com/css?family=Poppins:100,200,300,400,500,600,700,800,900&display=swap" rel="stylesheet">

    <title>Hexashop - Feedback</title>


    <!-- Additional CSS Files -->
    <link rel="stylesheet" type="text/css" href="~/Hexashop/assets/css/bootstrap.min.css">

    <link rel="stylesheet" type="text/css" href="~/Hexashop/assets/css/font-awesome.css">

    <link rel="stylesheet" href="~/Hexashop/assets/css/templatemo-hexashop.css">

    <link rel="stylesheet" href="~/Hexashop/assets/css/owl-carousel.css">

    <link rel="stylesheet" href="~/Hexashop/assets/css/lightbox.css">
    <!--

    TemplateMo 571 Hexashop

    https://templatemo.com/tm-571-hexashop

    -->
</head>

<body>

    <!-- ***** Preloader Start ***** -->
    <div id="preloader">
        <div class="jumper">
            <div></div>
            <div></div>
            <div></div>
        </div>
    </div>
    <!-- ***** Preloader End ***** -->
    <!-- ***** Header Area Start ***** -->
    <header class="header-area header-sticky">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <nav class="main-nav" data-aos="slide-down" data-aos-duration="1000" data-aos-delay="50">
                        <!-- ***** Logo Start ***** -->
                        <a href="index.html" class="logo">
                            <img src="~/Hexashop/assets/images/j.png" height="80px" width="120px">
                        </a>
                        <!-- ***** Logo End ***** -->
                        <!-- ***** Menu Start ***** -->
                        <ul class="nav">
                            <li class="scroll-to-section">  @Html.ActionLink("Home", "Index", "User")</li>
                            <li class="scroll-to-section">  @Html.ActionLink("Shop", "Index", "ShoopingProduct")</li>
                            <li class="scroll-to-section">  @Html.ActionLink("Food Court", "Index", "FoodA")</li>
                            <li class="scroll-to-section">  @Html.ActionLink("Gallery", "Index", "GalleriesUser")</li>
                            <li class="submenu">
                                <a href="javascript:;">Cinema</a>
                                <ul>

                                    <li>@Html.ActionLink("Movies", "Index", "UserMovies")</li>
                                    <li>@Html.ActionLink("Book Tickets", "Create", "UserTicket")</li>

                                </ul>
                            </li>
                            <li class="scroll-to-section">  @Html.ActionLink("FeedBack", "Create", "FeedbackUser")</li>
                            <li class="scroll-to-section">  @Html.ActionLink("Admin", "Login", "AdminDashboard")</li>
                        </ul>
                        <a class='menu-trigger'>
                            <span>Menu</span>
                        </a>
                        <!-- ***** Menu End ***** -->
                    </nav>
                </div>
            </div>
        </div>
    </header>
    <!-- ***** Header Area End ***** -->
    <!-- ***** Main Banner Area Start ***** -->
    <div class="page-heading about-page-heading" id="top">
        <div class="container">
            <div class="row">

            </div>
        </div>
    </div>
    <!-- ***** Main Banner Area End ***** -->
    <!-- ***** Contact Area Starts ***** -->
    <!-- ***** Contact Area Ends ***** -->
    <!-- ***** Subscribe Area Starts ***** -->
    <!-- ***** Subscribe Area Ends ***** -->
    @* User Ticket *@

    <div class="container">
        <div class="row">
            <div class="col-12">
                <div>
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>

    <footer>
        <div class="container">
            <div class="row">
                <div class="col-lg-3" data-aos="slide-right" data-aos-duration="1000">
                    <div class="first-item">
                        <div class="logo">
                            @*@foreach (var item in Model) {*@
                            <img src="~/Hexashop/assets/images/j.png" height="80px" width="120px">
                            @*  *@}
                        </div>
                        <ul>
                            <li><a href="#">Aptech Clifton,Karachi,Pakistan</a></li>
                            <li><a href="#">ABC@gmail.com</a></li>
                            <li><a href="#">03499234678</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-9 d-flex align-items-center justify-content-end" data-aos="slide-left" data-aos-duration="1000">
                    <div class="text-center">
                        <h3 class="text-light fw-light pb-3">Shopping &amp; Categories</h3>
                        <ul>
                            <li> @Html.ActionLink("Shops", "Index", "ShoopingProduct") </li>
                            <li> @Html.ActionLink("Food Courts", "Index", "FoodA") </li>
                            <li> @Html.ActionLink("Booking Tickets", "Index", "UserTicket") </li>
                            <li> @Html.ActionLink("Cinema Movies", "Index", "UserMovies") </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </footer>

    <!-- jQuery -->
    <script src="~/Hexashop/assets/js/jquery-2.1.0.min.js"></script>

    <!-- Bootstrap -->
    <script src="~/Hexashop/assets/js/popper.js"></script>
    <script src="~/Hexashop/assets/js/bootstrap.min.js"></script>

    <!-- Plugins -->
    <script src="~/Hexashop/assets/js/owl-carousel.js"></script>
    <script src="~/Hexashop/assets/js/accordions.js"></script>
    <script src="~/Hexashop/assets/js/datepicker.js"></script>
    <script src="~/Hexashop/assets/js/scrollreveal.min.js"></script>
    <script src="~/Hexashop/assets/js/waypoints.min.js"></script>
    <script src="~/Hexashop/assets/js/jquery.counterup.min.js"></script>
    <script src="~/Hexashop/assets/js/imgfix.min.js"></script>
    <script src="~/Hexashop/assets/js/slick.js"></script>
    <script src="~/Hexashop/assets/js/lightbox.js"></script>
    <script src="~/Hexashop/assets/js/isotope.js"></script>

    <!-- Global Init -->
    <script src="~/Hexashop/assets/js/custom.js"></script>

    <script>

        $(function () {
            var selectedClass = "";
            $("p").click(function () {
                selectedClass = $(this).attr("data-rel");
                $("#portfolio").fadeTo(50, 0.1);
                $("#portfolio div").not("." + selectedClass).fadeOut();
                setTimeout(function () {
                    $("." + selectedClass).fadeIn();
                    $("#portfolio").fadeTo(50, 1);
                }, 500);

            });
        });

    </script>
    @* Most important script *@
    @RenderSection("scripts", required: false)
</body>

</html>
